# AWS Configuration
aws_region = "us-east-1"  # Default from variables.tf
aws_profile = "default"   # Default from variables.tf

# Network Configuration
vpc_cidr = "10.0.0.0/16"  # Default from variables.tf
public_subnet_cidr = "10.0.1.0/24"  # Default from variables.tf
private_subnet_cidr = "10.0.2.0/24"  # Default from variables.tf
availability_zones = ["us-east-1a", "us-east-1b"]  # Default from variables.tf
health_check_path = "/"  # Default from variables.tf

# EC2 Configuration
ec2_instance_type = "t2.micro"  # Default from variables.tf
ec2_instance_name = "moveo-ec2"  # Default from variables.tf

# ALB Configuration
alb_name = "moveo-alb"  # Default from variables.tf

# Environment & Tagging
environment = "moveo"  # Default from variables.tf
tags = {  # Default from variables.tf
  Environment = "moveo"
  Project     = "devops-assignment"
}

# Security Configuration
enable_eip_for_ssh = true  # Default from variables.tf
allowed_ssh_cidr = null    # Default from variables.tf (will be overridden by EIP if enable_eip_for_ssh is true)

# SSH Public Keys (REPLACE THESE VALUES WITH YOUR ACTUAL KEYS)
# These are sensitive values and should be in terraform.tfvars, not here
# ssh_public_key_value = "REPLACE_WITH_EC2_SSH_PUBLIC_KEY"
# ssh_public_key_nat_value = "REPLACE_WITH_NAT_SSH_PUBLIC_KEY" 